<% content_for :title  do %>
  Edit Your Review
<% end %>
Edit your review:
  <%= simple_form_for @review do |f|%>
  <div class="form-card">
    <%= f.input :star, 'Stars', as: :radio_buttons, collection: [ 1, 2, 3, 4, 5] %>
    <%= f.input :content %>
    <%= f.button :submit, 'Submit Changes' %>
  </div>
<% end %>
